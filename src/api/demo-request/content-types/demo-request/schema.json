{
  "kind": "collectionType",
  "collectionName": "demo_requests",
  "info": {
    "singularName": "demo-request",
    "pluralName": "demo-requests",
    "displayName": "Demo Request",
    "description": "Demo form submissions from website"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "companyName": {
      "type": "string"
    },
    "message": {
      "type": "text"
    },
    "status": {
      "type": "enumeration",
      "enum": ["pending", "contacted", "converted", "rejected"],
      "default": "pending"
    },
    "notes": {
      "type": "text"
    },
    "source": {
      "type": "string",
      "default": "website"
    }
  }
}

