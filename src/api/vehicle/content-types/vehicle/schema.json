{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "Vehicle",
    "description": "Kurum Yönetimi - Araçlarım"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "institution": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::institution.institution",
      "required": true
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "plateNumber": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string",
      "required": true
    },
    "inspectionDate": {
      "type": "date",
      "required": false
    },
    "insurancePolicyDate": {
      "type": "date",
      "required": false
    },
    "usedBy": {
      "type": "string",
      "required": false
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company-profile.company-profile"
    }
  }
}



