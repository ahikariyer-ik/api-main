{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "releaseDate": {
      "type": "date"
    },
    "blog_tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-tag.blog-tag"
    },
    "content": {
      "type": "richtext"
    },
    "title": {
      "type": "string"
    }
  }
}
